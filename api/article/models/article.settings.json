{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title_FR": {
      "type": "string",
      "required": true
    },
    "title_ES": {
      "type": "string",
      "required": true
    },
    "pdf": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "media.image",
        "text.paragraph"
      ]
    },
    "definition": {
      "type": "component",
      "repeatable": true,
      "component": "definition.definition"
    },
    "routeName": {
      "type": "string"
    },
    "published": {
      "type": "boolean",
      "default": false
    }
  }
}
